{
  "version": 2,
  "builds": [
    {
      "src": "server.mjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.mjs"
    }
  ],
  "env": {
    "XRPL_NETWORK": "wss://s.devnet.rippletest.net:51233"
  },
  "functions": {
    "server.mjs": {
      "maxDuration": 30
    }
  }
}
